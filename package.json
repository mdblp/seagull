{
 "name": "seagull",
 "version": "0.4.1",
 "description": "API for managing user metadata.",
 "main": "lib/index.js",
 "directories": {
  "lib": "./lib"
 },
 "scripts": {
  "start": "node lib/index.js",
  "test": "mocha --exit",
  "lint": "jshint",
  "prebuild-doc": "mkdir -p doc/openapi",
  "prebuild-soup": "mkdir -p doc/soup",
  "build-doc": "swagger-jsdoc -o doc/${npm_package_name}-${npm_package_version}-swagger.json -d swaggerDef.js lib/*.js",
  "build-soup": "release-helper gen-dep-report doc/soup/${npm_package_name}-${npm_package_version}-soup.md"
 },
 "repository": "https://github.com/tidepool-org/seagull.git",
 "author": "Tidepool",
 "license": "BSD-2-Clause",
 "dependencies": {
  "amoeba": "0.8.1",
  "async": "3.1.0",
  "bunyan": "1.8.12",
  "crypto-js": "3.1.8",
  "hakken": "0.2.1",
  "lodash": "4.17.15",
  "mongojs": "3.1.0",
  "request": "2.88.0",
  "restify": "8.3.3",
  "restify-errors": "8.0.0",
  "tidepool-gatekeeper": "0.2.5",
  "user-api-client": "0.5.1"
 },
 "devDependencies": {
  "grunt": "1.0.4",
  "grunt-cli": "1.3.2",
  "grunt-contrib-jshint": "2.1.0",
  "grunt-contrib-uglify": "4.0.1",
  "grunt-docco2": "0.2.1",
  "grunt-mocha-test": "0.13.3",
  "grunt-shell-spawn": "0.4.0",
  "jshint": "2.10.2",
  "mocha": "6.2.0",
  "salinity": "0.0.8",
  "supertest": "4.0.2",
  "swagger-jsdoc": "^4.0.0"
 }
}
